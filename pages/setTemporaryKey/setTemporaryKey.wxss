@import '../../fonts/iconfont.wxss';
page,.container{
    height: 100%;
    background: #EDEDED;
}
.picker{
    background: #fff;
    padding: 0 60rpx;
    text-align:center;
    width: 100%;
    height: 280rpx;
    margin: 0 auto;
}
.picker-item{
    display: flex;
    align-items: center;
    justify-content: center;
}
.title{
    background: #fff;
    display: flex;
    justify-content: space-between;
    align-items:center;
    height: 100rpx;
    padding: 0 60rpx;
    border-bottom: 1rpx solid #ECECEC;
    color: #B8b8b8
}
.title-tow{
    margin-top: 20rpx;
}
.newKey{
    position: fixed;
    left: 50%;
    bottom: 100rpx;
    margin-left: -200rpx;
    width: 400rpx;
    height: 96rpx;
    background-color: #009CFF;
    border-radius: 48rpx;
    text-align: center;
    line-height: 96rpx;
    color: #fff;
}
.mincon{
    position: relative;
    width: 60%;
    margin: 10rpx auto;
}
.keyInput{
    border: 1rpx solid #009CFF;
    margin-top: 10rpx;
    border-radius: 20rpx;
    
    padding-left: 20rpx;
}
.icon-delete{
    position: absolute;
    right: 0;
    top: 0;
    font-size: 48rpx;
    z-index: 100;
}